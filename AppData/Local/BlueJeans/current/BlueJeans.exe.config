  <MediaClient OverrideMediaFacade="False" MediaFacadeName="Default" MediaFacadeFallbackOption="Default">
    <MediaFacades>
      <MediaFacade name="Citrix" assemblyFile="BlueJeans.MediaFacade.Citrix, Culture=neutral, PublicKeyToken=6f45126dc8090c6d" isRemoting="true" />
      <MediaFacade name="Loopback" assemblyFile="BlueJeans.MediaFacade.Loopback, Culture=neutral, PublicKeyToken=5ce34c5a25e4efad" isRemoting="true" />
    </MediaFacades>
  </MediaClient>
  <log4net>
    <appender name="ConsoleAppender" type="log4net.Appender.ConsoleAppender">
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%utcdate{yyyy-MM-ddTHH:mm:ss:ffff} | %-3thread | %-7level | %logger | --&gt; %message %newline" />
      </layout>
    </appender>
    <appender name="DebugAppender" type="log4net.Appender.DebugAppender">
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%utcdate{yyyy-MM-ddTHH:mm:ss:ffff} | %-3thread | %-7level | %logger | --&gt; %message %newline" />
      </layout>
    </appender>
    <appender name="RollingLogFileAppender" type="log4net.Appender.RollingFileAppender">
      <file />
      <appendToFile value="true" />
      <rollingStyle value="Composite" />
      <MaximumFileSize value="10MB" />
      <MaxSizeRollBackups value="10" />
      <PreserveLogFileNameExtension value="True" />
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%utcdate{yyyy-MM-ddTHH:mm:ss:ffff} | %-3thread | %-7level | %logger | --&gt; %message %newline" />
      </layout>
    </appender>
    <root>
      <level value="Info" />
      <appender-ref ref="RollingLogFileAppender" />
      <appender-ref ref="ConsoleAppender" />
      <!--<appender-ref ref="DebugAppender" />-->
    </root>
  </log4net>
  <system.diagnostics>
    <sources>
      <source name="ReactWindows" switchName="SourceSwitch" switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <add name="ReactNativeTraceListener" />
          <remove name="Default" />
        </listeners>
      </source>
    </sources>
    <switches>
      <add name="SourceSwitch" value="Warning" />
    </switches>
    <sharedListeners>
      <add name="ReactNativeTraceListener" type="BlueJeans.Config.ReactNativeTraceListener, BlueJeans" Source="ReactWindows" BaseFileName="RNWLog" />
    </sharedListeners>
    <trace autoflush="true" indentsize="4" />
  </system.diagnostics>
</configuration>
