      <setting name="LoggingOutputFileName" serializeAs="String">
        <value>BlueJeans-Win-App.log</value>
      </setting>
      <setting name="LoggingAutoFlush" serializeAs="String">
        <value>True</value>
      </setting>
      <setting name="LoggingLevel" serializeAs="String">
        <value>INFO</value>
      </setting>
      <setting name="OverrideAppVersion" serializeAs="String">
        <value>True</value>
